  },
  "homepage": "https://github.com/cecileanne/SwaddlesForWaddles#readme",
  "dependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jimp": "^0.9.3",
    "mongoose": "^5.8.1",
    "morgan": "^1.9.1",
    "react-leaf-carousel": "^2.0.0"
  }
